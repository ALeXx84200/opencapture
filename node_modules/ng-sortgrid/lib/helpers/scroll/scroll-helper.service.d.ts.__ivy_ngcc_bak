export interface ScrollPoints {
    top?: number;
    bottom?: number;
}
export declare class ScrollHelperService {
    private document;
    private window;
    private DEFAULT_SCROLLSPEED;
    private SCROLL_BUFFER;
    constructor(document: any);
    scrollIfNecessary(event: any, scrollPoints?: ScrollPoints, scrollSpeed?: number): void;
    private isTopScrollNeeded;
    private isBottomScrollNeeded;
}
